    traffic_insurance, business_insurance, vehicle_name
  </sql>
  <select id="selectByPrimaryKey" resultMap="BaseResultMap" parameterType="java.lang.Integer" >
    select 
    <include refid="Base_Column_List" />
    from ca_car_info
    where CAR_ID = #{carId,jdbcType=INTEGER}
  </select>
  <!-- 根据车辆id更新GPS相关信息 -->
  <update id="updateGpsInfo" parameterType="com.mljr.gps.entity.CarInfo">
    update ca_car_info
    <set >
      <if test="gpsDealer != null" >
        GPS_DEALER = #{gpsDealer,jdbcType=VARCHAR},
      </if>
      <if test="gpsNo != null" >
        GPS_NO = #{gpsNo,jdbcType=VARCHAR},
      </if>
      <if test="gpsInstallWay != null" >
        GPS_INSTALL_WAY = #{gpsInstallWay,jdbcType=INTEGER},
      </if>
    </set>
    where car_id = #{carId,jdbcType=INTEGER}
  </update>
