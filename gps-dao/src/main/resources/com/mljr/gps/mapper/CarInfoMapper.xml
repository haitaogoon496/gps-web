  <sql id="Base_Column_List" >
    CAR_ID, CAR_BRAND, CAR_SERIES, CAR_YEAR, CAR_GEARBOX, CAR_DISTON, CAR_STYLES, CAR_NEW_PRICE, 
    CAR_IDENTIFY, ENGINE_NO, CAR_COLOR, fuel_type, IS_OLD, CAR_MILES, CAR_FIRST_BOOK, 
    CAR_SALE_NAME, CAR_SALE_IDNO, REMAKES, A_CAR_CONDTION, A_CAR_PRICE, FLOATING_PRO, 
    A_CAR_PRICE_CHANGE, PLATE_NUMBER, MORTGAGE_DATE, OPEN_FARE, vin_identify_che300, 
    transfer_cnt, trans_city, is_match_che300, report_url, trans_city_id, vin_identify_che300_id, 
